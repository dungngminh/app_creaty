    "version": "0.2.0",
    "configurations": [
  
      {
        "name": "Launch app (Debug)",
        "request": "launch",
        "type": "dart",
        "program": "lib/main.dart",
        "toolArgs": [
          "--dart-define-from-file=env.json"
        ]
      },
      {
        "name": "Launch app (Profile)",
        "request": "launch",
        "type": "dart",
        "flutterMode": "profile",
        "program": "lib/main.dart",
        "toolArgs": [
          "--dart-define-from-file=env.json"
        ]
      },
      {
        "name": "Launch app (Release)",
        "request": "launch",
        "type": "dart",
        "flutterMode": "release",
        "program": "lib/main.dart",
        "toolArgs": [
          "--dart-define-from-file=env.json"
        ]
      },
      {
        "name": "Dart: Run all Tests",
        "type": "dart",
        "request": "launch",
        "program": "./test/",
      },
    ]
  }
  